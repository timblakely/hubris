{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cargo",
      "command": "xtask",
      "problemMatcher": [
        "$rustc"
      ],
      "label": "xtask build clock-inspect",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "options": {
        "cwd": "${workspaceRoot}/../../../"
      },
      "args": [
        "build",
        "app/bldc/app.toml",
        "clock_inspect"
      ]
    }
  ]
}
