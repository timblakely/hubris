// API to manipulate PWM output.

Interface(
    name: "Pwm",
    ops: {
        "set_duty": (
            args: {
                "phase_a": "u16",
                "phase_b": "u16",
                "phase_c": "u16",
            },
            reply: Result(
                ok: "()",
                err: CLike("PwmError"),
            ),
        ),
    },
)
