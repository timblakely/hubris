{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "xtask",
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo xtask dist",
			"group": "build",
			"args": [
				"dist",
				"app/demo-stm32h7-nucleo/app-h743.toml"
			]
		},
		{
			"type": "cargo",
			"command": "xtask",
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo xtask build ",
			"group": {
				"kind": "build"
			},
			"args": [
				"dist",
				"app/demo-stm32h7-nucleo/app-h743.toml"
			]
		},
		{
			"type": "cargo",
			"command": "xtask",
			"problemMatcher": [
				"$rustc"
			],
			"label": "xtask dist bldc",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"args": [
				"dist",
				"bldc/app/app.toml"
			]
		},
		{
			"type": "cargo",
			"command": "xtask",
			"problemMatcher": [
				"$rustc"
			],
			"label": "xtask build clock-inspect",
			"group": "build",
			"args": [
				"build",
				"bldc/app/app.toml",
				"clock_inspect"
			]
		}
	]
}
